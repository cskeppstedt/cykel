<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, user-scalable=0; maximum-scale=1.0" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black" />
    <title>@(ViewBag.Title ?? "cykel.chsk.se")</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js", "http://maps.google.com/maps/api/js?libraries=geometry&sensor=true")
    @Scripts.Render("~/Scripts/js")
    <script src="signalr/hubs" type="text/javascript"></script>
</head>
<body>
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
